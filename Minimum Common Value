class Solution(object):
    def getCommon(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: int
        """
        s=set(nums1)
        m=set(nums2)
        l=list(s.intersection(m))
        if l:
            return min(l)
        else:
            return -1
        
