class Solution(object):
    def isReachable(self, targetX, targetY):
        """
        :type targetX: int
        :type targetY: int
        :rtype: bool
        """
        
        
        import math
        
        while True:
            def Log2(x):
                if x == 0:
                    return false

                return (math.log10(x) /
                        math.log10(2))


 
            def isPowerOfTwo(n):
                return (math.ceil(Log2(n)) ==
                        math.floor(Log2(n)))

            if targetX==targetY:
                return isPowerOfTwo(targetX)
            if targetX>targetY:
                targetX-=targetY
            else:
                targetY-=targetX
            
            
            
            
                
            
                
                
        
